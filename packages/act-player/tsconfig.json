{
  "compilerOptions": {
    "target": "esnext",
    "strict": true,
    "baseUrl": "./src/",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "types": ["react/next", "react-dom/next"]
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
